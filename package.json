{
  "publisher": "yjhtry",
  "name": "open-git-repo",
  "displayName": "open-git-repo",
  "version": "0.0.10",
  "packageManager": "pnpm@8.15.4",
  "description": "open github or gitlab repo in browser",
  "author": "yjhtry <860622588@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/yjhtry/open-git-repo#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yjhtry/open-git-repo"
  },
  "bugs": {
    "url": "https://github.com/yjhtry/open-git-repo/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.87.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.12.7",
    "@types/vscode": "^1.88.0",
    "@vscode/vsce": "^2.25.0",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "pnpm": "^9.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "commands": [
    {
      "command": "open-git-repo.open"
    }
  ]
}
