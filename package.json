{
  "publisher": "yjhtry",
  "name": "open-git-repo",
  "displayName": "open-git-repo",
  "version": "0.0.5",
  "packageManager": "pnpm@8.15.1",
  "description": "open github or gitlab repo in browser",
  "author": "yjhtry <860622588@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/yjhtry/open-git-repo#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yjhtry/open-git-repo"
  },
  "bugs": {
    "url": "https://github.com/yjhtry/open-git-repo/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.11.17",
    "@types/vscode": "^1.86.0",
    "@vscode/vsce": "^2.23.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "pnpm": "^8.15.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vitest": "^1.2.2"
  },
  "commands": [
    {
      "command": "open-git-repo.open"
    }
  ]
}
